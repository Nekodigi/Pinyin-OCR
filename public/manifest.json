{
  "manifest_version": 3,
  "name": "Pinyin OCR",
  "description": "An extension that uses OCR to convert a image to Pinyin",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "host_permissions": ["https://github-readme-stats.vercel.app/*"]
}
